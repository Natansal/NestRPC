{
   "name": "@nestjs-rpc/client",
   "version": "1.0.4",
   "keywords": [
      "nestjs",
      "nest",
      "rpc",
      "remote-procedure-call",
      "nestjs-rpc",
      "client",
      "http",
      "fetch",
      "batching",
      "batched-requests",
      "proxy",
      "typescript",
      "type-safe",
      "typed-client",
      "api-client",
      "sdk"
   ],
   "main": "dist/cjs/index.js",
   "module": "dist/esm/index.js",
