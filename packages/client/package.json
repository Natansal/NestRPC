      "nestjs-rpc",
      "client",
      "http",
      "fetch",
      "batching",
      "batched-requests",
      "proxy",
      "typescript",
      "type-safe",
      "typed-client",
      "api-client",
      "sdk"
   ],
   "main": "dist/cjs/index.js",
